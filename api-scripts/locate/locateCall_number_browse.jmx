                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">import groovy.json.JsonSlurper

// Get the JSON response
String response = prev.getResponseDataAsString()

// Parse JSON
def json = new JsonSlurper().parseText(response)

// Initialize variables for storing the extracted values
String fullCallNumber = null
String instanceId = null
List&lt;String&gt; isbns = []
String effectiveShelvingOrder = null

// Find a random item where instance.isbns contains exactly one record
def matchingItems = json.items.findAll { it.instance?.isbns?.size() == 1 }
if (matchingItems) {
    def randomItem = matchingItems[new Random().nextInt(matchingItems.size())]
    
    fullCallNumber = randomItem.fullCallNumber
    instanceId = randomItem.instance?.id
    isbns = randomItem.instance?.isbns
    effectiveShelvingOrder = randomItem.instance?.items?.find()?.effectiveShelvingOrder
}

// Set variables in JMeter
vars.put(&quot;fullCallNumber&quot;, fullCallNumber ?: &quot;&quot;)
vars.put(&quot;instanceId&quot;, instanceId ?: &quot;&quot;)
vars.put(&quot;isbns&quot;, isbns ? isbns.join(&quot;,&quot;) : &quot;&quot;)
vars.put(&quot;effectiveShelvingOrder&quot;, effectiveShelvingOrder ?: &quot;&quot;)

//// Log for debugging
//log.info(&quot;Extracted fullCallNumber: &quot; + fullCallNumber)
//log.info(&quot;Extracted instanceId: &quot; + instanceId)
//log.info(&quot;Extracted isbns: &quot; + isbns)
//log.info(&quot;Extracted effectiveShelvingOrder: &quot; + effectiveShelvingOrder)
</stringProp>
                <stringProp name="TestPlan.comments">This extractor takes only instances where instance?.isbns?.size == 1. Extracted vars used in TC_Open-Instance-page</stringProp>
