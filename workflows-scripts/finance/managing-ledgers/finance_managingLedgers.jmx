            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">def distributions = vars.get(&quot;DISTRIBUTION&quot;).split(&quot;-&quot;);
String[] flows = [&quot;ledgers_create&quot;, &quot;ledgers_view&quot;, &quot;ledgers_edit&quot;, &quot;ledgers_delete&quot;];
def selected = 0;
def leftBorder = 0;
def randomNumber = new Random().nextInt(100);
for (distribution in distributions) {
  def rightBorder = distribution.toInteger() + leftBorder;
  if((randomNumber &gt;= leftBorder) &amp;&amp; (randomNumber &lt; rightBorder)) {
  	break;
  }
  leftBorder = rightBorder;
  selected++;
}

vars.put(&quot;flow&quot;, flows[selected]);</stringProp>
          </JSR223PreProcessor>
