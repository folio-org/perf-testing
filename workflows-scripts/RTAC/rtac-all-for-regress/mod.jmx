          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import org.apache.jmeter.services.FileServer;
import java.io.FileWriter;
import java.io.IOException;

String xokapitoken = vars.get(&quot;x-okapi-token-central&quot;);
String current_userID = vars.get(&quot;central_userID&quot;);
String servicePointId = vars.get(&quot;central_servicePointId&quot;);
String hostname = vars.get(&quot;hostName&quot;);
String tenant = vars.get(&quot;central_tenant&quot;);
String separator = &quot;,&quot;;

try {
    FileWriter writer = new FileWriter(FileServer.getFileServer().getBaseDir() + &quot;/jmeter-supported-data/UsersInfo_central.csv&quot;, true);
    writer.append(xokapitoken + separator + current_userID + separator + servicePointId + separator + hostname + separator + tenant + &quot;\n&quot;);
    writer.close();
} catch (IOException ex) {
    log.error(&quot;Failed to write to CSV file: &quot; + ex.getMessage());
}
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
